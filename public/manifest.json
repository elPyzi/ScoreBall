{
  "name": "ScoreBall",
  "description": "Extension about football statistic and view in real time matches",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png"
  },
  "icons":{
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "permissions": [],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}
